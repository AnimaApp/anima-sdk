{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@animaapp/anima-sdk#dev": {
      "outputs": [
        "dist/**"
      ]
    },
    "@animaapp/anima-sdk-react#dev": {
      "dependsOn": [
        "@animaapp/anima-sdk#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@animaapp/anima-sdk-react#build": {
      "dependsOn": [
        "@animaapp/anima-sdk#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "publish": {
      "dependsOn": [
        "^publish"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "lint": {}
  }
}